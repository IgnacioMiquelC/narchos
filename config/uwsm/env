# Changes require a relaunch of Hyprland to take effect.

# Ensure narchos bins are in the path
export NARCHOS_PATH=$HOME/.local/share/narchos
export PATH=$NARCHOS_PATH/bin/:$PATH

# Set default terminal and editor
source ~/.config/uwsm/default

# Activate mise if present on the system
narchos-cmd-present mise && eval "$(mise activate bash)"
